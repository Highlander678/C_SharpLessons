<?xml version="1.0" encoding="utf-8"?>
<!--Телефонная_книга - это корневой элемент XML-документа:-->
<Телефонная_книга>
  <!--Элемент СТРОКА содержит атрибут Контакт и два вложенных элемента-->
  <СТРОКА Контакт="Олег">
    <Домашний_телефон>236-23-67</Домашний_телефон>
    <Мобильный_телефон>+7(495)625-31-43</Мобильный_телефон>
  </СТРОКА>
  <!--Атрибут Контакт имеет значение 'Прогноз погоды':-->
  <СТРОКА Контакт="Прогноз погоды">
    <Домашний_телефон>001</Домашний_телефон>
    <Мобильный_телефон></Мобильный_телефон>
  </СТРОКА>
  <!--Поскольку каждый элемент Контакт имеет атрибут и два вложенных=>-->
  <СТРОКА Контакт="Борис Григорьевич">
    <Домашний_телефон>402-12-45</Домашний_телефон>
    <Мобильный_телефон>+7(495)536-79-94</Мобильный_телефон>
  </СТРОКА>
  <!--=> элемента, в MS Excel отобразится таблица с тремя колонками-->
  <СТРОКА Контакт="Света">
    <Домашний_телефон></Домашний_телефон>
    <Мобильный_телефон>+7(495)615-24-41</Мобильный_телефон>
  </СТРОКА>
</Телефонная_книга>